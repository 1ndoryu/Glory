{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "node",
        "types": ["node"],
        "esModuleInterop": true,
        "strict": true,
        "noEmit": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "skipLibCheck": true
    },
    "include": ["scripts"]
}
